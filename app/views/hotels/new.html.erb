<h1>Create a new Hotel</h1>

<h3>fill in the details, below:</h3>


<%= form_for(@hotel) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :country_code %>
	<%= f.text_field :country_code %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.submit %>
<% end %>